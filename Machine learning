    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Parámetros\n",
        "mu = 0\n",
        "sigma = 0.2   # porque sigma^2 = 0.04\n",
        "n_sim = 10000\n",
        "\n",
        "# Simulación\n",
        "errores = np.random.normal(mu, sigma, n_sim)\n",
        "\n",
        "# Histograma\n",
        "plt.hist(errores, bins=40, density=True)\n",
        "plt.xlabel(\"Error de medición\")\n",
        "plt.ylabel(\"Densidad\")\n",
        "plt.title(\"Simulación de 10,000 errores ~ N(0, 0.04)\")\n",
        "plt.show()\n",
        "\n",
        "# Estadísticas simuladas\n",
        "print(\"Media simulada:\", np.mean(errores))\n",
        "print(\"Desviación estándar simulada:\", np.std(errores))\n"
      ],
