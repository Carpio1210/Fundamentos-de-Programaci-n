      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Parámetros\n",
        "n = 21          # número de monedas\n",
        "p = 0.7         # probabilidad de cara\n",
        "experimentos = 10000\n",
        "\n",
        "\n",
        "# Simulación binomial\n",
        "resultados = np.random.binomial(n, p, experimentos)\n",
        "\n",
        "# Graficar histograma\n",
        "plt.hist(resultados, bins=range(n+2), density=True)\n",
        "plt.xlabel(\"Número de caras\")\n",
        "plt.ylabel(\"Frecuencia relativa\")\n",
        "plt.title(\"Simulación Binomial (n=21, p=0.7)\")\n",
        "plt.show()\n",
        "\n",
        "# Mostrar promedio experimental\n",
        "print(\"Promedio experimental:\", np.mean(resultados))\n",
        "print(\"Valor esperado teórico:\", n*p)\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.stats import poisson\n",
        "\n",
        "# Parámetro\n",
        "lambda_ = 18   # 6 peticiones por minuto × 3 minutos\n",
        "\n",
        "# Valores de n\n",
        "n_values = np.arange(0, 31)\n",
        "\n",
        "# Probabilidades teóricas\n",
        "probabilidades = poisson.pmf(n_values, lambda_)\n",
        "\n",
        "# Graficar\n",
        "plt.bar(n_values, probabilidades)\n",
        "plt.xlabel(\"Número de peticiones (n)\")\n",
        "plt.ylabel(\"P(X = n)\")\n",
        "plt.title(\"Distribución Poisson (λ = 18)\")\n",
        "plt.show()\n",
        "\n",
        "# Ejemplo: probabilidad de exactamente 18 peticiones\n",
        "print(\"P(X = 18) =\", poisson.pmf(18, lambda_))\n"
      ],
